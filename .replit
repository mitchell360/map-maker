modules = ["web", "python-3.11"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["gitFull", "playwright-driver", "nspr", "nss", "dbus", "at-spi2-atk", "cups", "xorg.libxcb", "xorg.libXcomposite", "xorg.libXdamage", "xorg.libXfixes", "libxkbcommon", "libdrm", "mesa", "alsa-lib", "pango", "cairo", "chromium"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Map Server"

[[workflows.workflow]]
name = "Map Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 35015
externalPort = 3001

[[ports]]
localPort = 45975
externalPort = 3000

[deployment]
deploymentTarget = "static"
run = ["python3", "server.py"]
publicDir = "."
